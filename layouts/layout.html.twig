  <body>
    {% block body %}
      {% block header %}
        <header class="header-8">
          <div class="container">
            <div class="row">
              <div class="navbar col-sm-12" role="navigation">
                <div class="navbar-header">
                  <button type="button" class="navbar-toggle"></button>
                  <a class="brand" href="index.html#">
